{
    "name": "90storezon/calculator-website",
    "description": "A comprehensive calculator website with 50+ calculators including scientific, financial, health, and math calculators.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "90 Store Zone Team",
            "email": "dev@90storezon.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Calculator\\": "includes/"
        },
        "files": [
            "includes/functions.php"
        ]
    },
    "scripts": {
        "test": "phpunit tests/",
        "lint": "phpcs --standard=PSR12 includes/ calculators/",
        "format": "phpcbf --standard=PSR12 includes/ calculators/"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "calculator",
        "scientific-calculator",
        "financial-calculator",
        "health-calculator",
        "math-tools",
        "web-calculator"
    ],
    "support": {
        "email": "support@90storezon.com",
        "issues": "https://github.com/90storezon/calculator/issues",
        "source": "https://github.com/90storezon/calculator"
    }
}