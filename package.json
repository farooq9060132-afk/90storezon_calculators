{
  "name": "90storezon-calculator-website",
  "version": "1.0.0",
  "description": "Comprehensive calculator website with 50+ calculators",
  "main": "script.js",
  "scripts": {
    "dev": "lite-server",
    "build:css": "postcss assets/css/*.css -d dist/css/",
    "build:js": "babel assets/js/ -d dist/js/ --presets=@babel/preset-env",
    "minify:css": "css-minify -f assets/css/style.css -o dist/css/",
    "minify:js": "uglifyjs assets/js/*.js -c -m -o dist/js/script.min.js",
    "lint:js": "eslint assets/js/",
    "lint:css": "stylelint assets/css/*.css",
    "test": "jest",
    "prettier": "prettier --write \"**/*.{js,css,html,php}\"",
    "watch:css": "nodemon -e css -x \"npm run build:css\""
  },
  "keywords": [
    "calculator",
    "scientific-calculator",
    "financial-calculator",
    "math-tools",
    "web-app"
  ],
  "author": "90 Store Zone Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "autoprefixer": "^10.4.16",
    "css-minify": "^2.0.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "lite-server": "^2.6.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "uglify-js": "^3.17.4"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/90storezon/calculator-website.git"
  },
  "bugs": {
    "url": "https://github.com/90storezon/calculator-website/issues"
  },
  "homepage": "https://90storezon.com"
}